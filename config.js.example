{
    "i2c": {
        "device": "/dev/i2c-1"
    },
    "serial": {
        "device": "/dev/ttyACM0"
    },
    "network": {
        "ipaddr": "192.168.1.30"
    },
    "datalogger": {
        "logfile": "false"
        "logstash": "true"
    },
    "camera": {
	"config": "0",
        "command": "/usr/local/bin/mjpg_streamer",
        "output": "output_http.so -w /root/mjpg-streamer/mjpg-streamer-experimental/www",
        "input": "input_raspicam.so -x 1366 -y 768 -vf -hf",
        "kill": "kill -9 `pidof mjpg_streamer`",
    }
}
